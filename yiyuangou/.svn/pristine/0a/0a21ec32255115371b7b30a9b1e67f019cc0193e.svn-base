<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:id="@+id/participation"
        android:clickable="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_margin="10dp">

            <Button
                android:layout_width="110dp"
                android:layout_height="wrap_content"
                android:background="@null"
                android:drawableLeft="@mipmap/bg_1"
                android:text="参与记录" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerHorizontal="true"
                android:background="@mipmap/bg_4" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/bgwhlie" />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/f5_include_xingyunji_lulayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginLeft="10dp">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_margin="10dp">
            <Button
                android:layout_width="110dp"
                android:layout_height="wrap_content"
                android:background="@null"
                android:drawableLeft="@mipmap/bg_2"
                android:text="幸运记录"/>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerHorizontal="true"
                android:background="@mipmap/bg_4"
                />
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/bgwhlie"/>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/f5_include_the_sun_lulayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginLeft="10dp">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_margin="10dp">
            <Button
                android:layout_width="110dp"
                android:layout_height="wrap_content"
                android:background="@null"
                android:drawableLeft="@mipmap/bg_3"
                android:text="我的晒单"/>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerHorizontal="true"
                android:background="@mipmap/bg_4"
                />
        </RelativeLayout>
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:background="@color/bgwhlie"/>
    <LinearLayout
        android:id="@+id/my_news_layout_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:clickable="true"
        android:layout_marginLeft="10dp">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_margin="10dp">
            <Button
                android:id="@+id/my_news"
                android:layout_width="110dp"
                android:layout_height="wrap_content"
                android:background="@null"
                android:drawableLeft="@mipmap/bg_3"
                android:text="我的消息"/>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerHorizontal="true"
                android:background="@mipmap/bg_4"
                />
        </RelativeLayout>
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/bgwhlie"/>
    <LinearLayout
        android:id="@+id/fragment_body_dizhi"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:clickable="true"
        android:layout_marginLeft="10dp">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_margin="10dp">
            <Button
                android:layout_width="140dp"
                android:layout_height="wrap_content"
                android:background="@null"
                android:drawableLeft="@mipmap/bg_3"
                android:text="收货地址管理"/>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerHorizontal="true"
                android:background="@mipmap/bg_4"
                />
        </RelativeLayout>
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/bgwhlie"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginLeft="10dp">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_margin="10dp">
            <Button
                android:layout_width="165dp"
                android:layout_height="wrap_content"
                android:background="@null"
                android:drawableLeft="@mipmap/bg_3"
                android:text="开通中奖短信提醒"/>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerHorizontal="true"
                android:background="@mipmap/bg_4"
                />
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/bgwhlie"
        android:orientation="horizontal">


    </LinearLayout>

</LinearLayout>